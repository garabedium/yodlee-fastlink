<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Yodlee FastLink Integration</title>
  </head>
  <body>
    <h1>Hello World</h1>

  <form action="<%= @NODE_URL %>"
      method="POST"
      name="rsessionPost"
      id="rsessionPost">
    <input type="text" name="app" value="10003600" />
    <input type="text" name="rsession" value="<%= @USER_SESSION %>"/>
    <input type="text" name="token" value="<%= @ACCESS_TOKEN %>" />
    <input type="text" name="redirectReq" value="true"/>
  </form>
  <script>
    let form = document.getElementById('rsessionPost');
    form.submit();
  </script>

  </body>
</html>